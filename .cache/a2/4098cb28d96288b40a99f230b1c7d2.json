-left: 4rem;\r\n    height: 4rem;\r\n    border-radius: 4rem;\r\n    transition: letter-spacing 0.2s linear;\r\n}\r\n.contact:hover{\r\n    letter-spacing: 2px;\r\n}\r\n/* footer */\r\nfooter{\r\n    min-height: 10vh;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    opacity: 0.5;\r\n}\r\n/* main */\r\n.container{\r\n    min-height: 60vh;\r\n    margin: 2rem auto;\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n.container .left{\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content:space-around;\r\n}\r\n.container .left .title{\r\n    font-size: 8rem;\r\n    font-weight: bolder;\r\n    animation: showTitle 2s linear forwards;\r\n}\r\n@keyframes showTitle {\r\n    from{\r\n        /* color: #8f94fb; */\r\n        opacity: 0.2;\r\n        letter-spacing: 4px;\r\n    }\r\n    to{\r\n        color: #000;\r\n        opacity: 1;\r\n        letter-spacing: 0;\r\n    }\r\n}\r\n.left .slogan{\r\n    width: 24rem;\r\n    font-size: 2rem;\r\n}\r\n.left .try{\r\n    width: 12rem;\r\n    height: 4rem;\r\n    text-align: center;\r\n    border: 3px solid #000;\r\n    border-radius: 3rem;\r\n    font-size: 1.5rem;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    transition: 0.2s linear;\r\n}\r\n.left .imgType{\r\n    opacity: 0.5;\r\n}\r\n.left .try:hover{\r\n    background-color:#000;\r\n    color:#fff;\r\n}\r\n.container .right{\r\n    display: flex;\r\n    align-items: center;\r\n    color: white;\r\n}\r\n.blur img{\r\n    width: 24vw;\r\n    transform: translateX(12vw);\r\n    animation: toHide 2s linear forwards;\r\n    animation-delay: 0.5s;\r\n    backdrop-filter: brightness(0);\r\n}\r\n.clear img{\r\n    width: 40vw;\r\n    opacity: 0;\r\n    animation: toShow 2s linear forwards;\r\n    animation-delay: 1s;\r\n}\r\n@keyframes toShow {\r\n    from {\r\n        opacity: 0;\r\n    }\r\n    to {\r\n        opacity: 1;\r\n    }\r\n}\r\n@keyframes toHide {\r\n    from {\r\n        opacity: 1;\r\n    }\r\n    to {\r\n        opacity: 0.6;\r\n    }\r\n}\r\n\r\n.next{\r\n    margin-top: 12vh;\r\n    text-align: center;\r\n    color: #000;\r\n    height: 18vh;\r\n    font-size: 10rem;\r\n    animation: toNext 2s linear infinite;\r\n}\r\n@keyframes toNext {\r\n    from {\r\n        opacity: 0.4;\r\n    }\r\n    to{\r\n        opacity: 1;\r\n        transform: translateY(5vh);\r\n    }\r\n}\r\n\r\n/* user  */\r\n.use{\r\n    /* border-top: 1px solid #000; */\r\n}\r\n.use .main{\r\n    /* background-color: #ffffff80; */\r\n    border-radius: 4rem;\r\n    min-height: 50vh;\r\n    padding: 2rem 0;\r\n    \r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content:space-between;\r\n    align-items:center;\r\n}\r\n.head{\r\n    min-height: 4vh;\r\n    display:flex;\r\n    justify-content: space-between;\r\n    align-items:center;\r\n    margin: 2rem 0;\r\n    font-size: 2rem;\r\n}\r\n.head .left{\r\n    display:flex;\r\n    align-items:center;\r\n}\r\n.add{\r\n    border: 4px solid #000;\r\n    border-radius: 2rem;\r\n    font-weight: bolder;\r\n    font-size: 2rem;\r\n    height: 2rem;\r\n    width: 2rem;\r\n    display: flex;\r\n    justify-content:center;\r\n    align-items:center;\r\n}\r\n.head .left .title{\r\n    margin-left: 1rem;\r\n}\r\n.head .right{\r\n    border-bottom: 2px solid #00000080;\r\n}\r\n.process{\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n.processItem{\r\n    color: #00000080;\r\n    margin-left: 4rem;\r\n}\r\n/* 处理区域 */\r\n.upload2res{\r\n    flex: 1;\r\n    display: flex;\r\n    justify-content:space-around;\r\n    align-items:center;\r\n}\r\n.upload{\r\n    background-color: #fff;\r\n    border-radius: 2rem;\r\n    box-shadow: -1rem 1rem 2rem #8f94fb80;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content:space-around;\r\n    align-items:center;\r\n    min-height: 40vh;\r\n}\r\n.top{\r\n    margin: 4rem;\r\n    \r\n    border: 4px dashed #8f94fb80;